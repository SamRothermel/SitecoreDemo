@using (Html.BeginRouteForm(Sitecore.Mvc.Configuration.MvcSettings.SitecoreRouteName, FormMethod.Post, new { enctype="multipart/form-data" }))
{
    @Html.Sitecore().FormHandler("DevTools", "ProcessImport")
    <div class="form-group">
        <input type="file" class="form-control" name="file" placeholder="Xml File">
    </div>
    <button type="submit" class="btn btn-default">Submit</button>
}
